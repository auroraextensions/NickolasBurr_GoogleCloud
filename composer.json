{
  "name": "nickolasburr/googlecloudphpautoloader",
  "description": "Autoload Google Cloud PHP classes for use within Magento 1.x.",
  "type": "magento-module",
  "license": "MIT",
  "keywords": [
    "google-cloud",
    "google-cloud-php",
    "magento"
  ],
  "authors": [
    {
      "name": "Nickolas Burr",
      "email": "nickolasburr@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "mnsami/composer-custom-directory-installer": "1.1.*",
    "google/cloud-core": "~1.23"
  },
  "config": {
    "vendor-dir": "./lib/GoogleCloud/"
  },
  "extra": {
    "installer-paths": {
      "./lib/GoogleCloud/core": ["google/cloud-core"]
    }
  }
}
